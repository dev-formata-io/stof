#[test]
fn null_or() {
    let x = null;
    x = or(x, 10);
    assertEq(x, 10);

    x = or(x, 20);
    assertEq(x, 10);
}

#[test]
fn null_boxed_or(): void {
    let x = box(null);
    x = or(x, 10);
    assertEq(x, 10);
    x = or(x, 22);
    assertEq(x, 10);
}

#[test]
fn null_or_many() {
    let x = null;
    let y = or(x, null, null, null, 10, 100);
    assertEq(x, null);
    assertEq(y, 10);
}
