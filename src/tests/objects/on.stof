#[test]
fn on_created() {
    let a = new {};
    let b = new {} on a;
    assertEq(b.parent(), a);
}

sub: {}
#[test('sub')]
fn on_field(): str {
    let a = new {
        message: 'hello'
    } on self.sub;
    assertEq(a.parent().name(), self.sub.name());
    return self.sub.name();
}
